{
  "manifest_version": 3,
  "name": "Pomofocus Quality of Life",
  "version": "1.0.0",
  "description": "Extra features for pomofocus.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "permissions": [
    "tabs", "activeTab", "scripting", "storage"
  ],
  "host_permissions": [
    "https://pomofocus.io/*",
    "https://todoist.com/*"
  ]
}
